<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

        <LinearLayout
            android:gravity="center"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_1">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_2">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_3">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:gravity="center"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_4">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_5">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_6">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_7">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_8">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_9">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:tag="kissDatePicker_digit_apply"
                    android:gravity="center|center_vertical"
                    android:clickable="false"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center">
                    <ImageView
                        android:duplicateParentState="true"
                        android:clickable="false"
                        android:src="@drawable/aw_done"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />
                </RelativeLayout>
                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center|center_vertical"
                    android:tag="kissDatePicker_digit_0">
                    <TextView
                        android:tag="value"
                        style="@style/kissDatePicker.TextViewDigit"
                        android:text="0"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/kissDatePicker.Layout"
                    android:tag="kissDatePicker_digit_back"
                    android:gravity="center|center_vertical"
                    android:clickable="true"
                    android:background="@drawable/kissdatepicker_second"
                    android:layout_gravity="center">
                    <ImageView
                        android:duplicateParentState="true"
                        android:clickable="false"
                        android:src="@drawable/aw_back"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />
                </RelativeLayout>
            </LinearLayout>


        </LinearLayout>



</LinearLayout>
